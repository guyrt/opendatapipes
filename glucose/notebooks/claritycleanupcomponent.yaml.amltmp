$schema: https://azuremlschemas.azureedge.net/latest/commandComponent.schema.json
type: command

name: clarity_cleanup
display_name: ClarityCleanup
version: 1

inputs:
  raw_clarity_data:
    type: uri_folder

outputs:
  clean_clarity_data:
    type: uri_file

code: .

environment: 
  image: python

command: >-
  python clarityCleanup.py --componentA_input ${{inputs.component_a_input}} --componentA_output ${{outputs.component_a_output}}